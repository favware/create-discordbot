{
  "name": "@favware/create-discordbot",
  "version": "1.0.0",
  "description": "CLI for easily bootstrapping a DiscordJS bot with the Commando framework",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "bin": {
    "create-discordbot": "./dist/index.js"
  },
  "author": "@favware",
  "license": "MIT",
  "homepage": "https://favware.tech/creatediscordbot",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/create-discordbot.git"
  },
  "bugs": {
    "url": "https://github.com/favware/create-discordbot/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "tslint -p . -c ./tslint.json --format verbose --fix",
    "test": "jest",
    "start": "ts-node ./lib/index.ts",
    "prebuild": "yarn lint && yarn test",
    "build": "tsc",
    "postbuild": "yarn terser",
    "prepublishOnly": "yarn build",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "terser": "terser-folder ./dist -eo ./dist -x .js"
  },
  "keywords": [
    "discord",
    "create",
    "bootstrap",
    "commando",
    "favware"
  ],
  "dependencies": {
    "chalk": "^2.4.2",
    "common-tags": "^1.8.0",
    "execa": "^1.0.0",
    "git-config-path": "^2.0.0",
    "github-username": "^5.0.0",
    "globby": "^9.2.0",
    "handlebars": "^4.1.2",
    "make-dir": "^3.0.0",
    "ora": "^3.4.0",
    "p-each-series": "^2.1.0",
    "parse-git-config": "^3.0.0",
    "tslib": "^1.9.3",
    "which": "^1.3.1",
    "yargs-interactive": "^2.1.0"
  },
  "devDependencies": {
    "@favware/tslint-config": "^1.0.1",
    "@types/common-tags": "^1.8.0",
    "@types/execa": "^0.9.0",
    "@types/git-config-path": "^2.0.0",
    "@types/jest": "^24.0.13",
    "@types/mock-fs": "^3.6.30",
    "@types/node": "^12.0.4",
    "@types/parse-git-config": "^3.0.0",
    "@types/which": "^1.3.1",
    "@types/yargs-interactive": "^2.0.0",
    "jest": "^24.8.0",
    "mock-fs": "^4.10.0",
    "rimraf": "^2.6.3",
    "terser-folder": "^2.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "files": [
    "dist"
  ]
}
