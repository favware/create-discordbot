{
  "name": "create-djsbot",
  "version": "2.1.0",
  "description": "CLI for easily bootstrapping a DiscordJS bot with the Commando framework",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "bin": {
    "create-djsbot": "./dist/index.js"
  },
  "author": "@favware",
  "license": "MIT",
  "homepage": "https://favware.tech/createdjsbot",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/create-djsbot.git"
  },
  "bugs": {
    "url": "https://github.com/favware/create-djsbot/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint lib test --ext js,ts --fix",
    "test": "jest",
    "start": "ts-node ./lib/index.ts",
    "prebuild": "yarn lint",
    "build": "tsc",
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "up": "yarn upgrade-interactive --latest",
    "standard": "yarn standard-version"
  },
  "keywords": [
    "discord",
    "create",
    "bootstrap",
    "commando",
    "favware"
  ],
  "dependencies": {
    "chalk": "^3.0.0",
    "common-tags": "^1.8.0",
    "execa": "^3.4.0",
    "git-config-path": "^2.0.0",
    "github-username": "^5.0.1",
    "globby": "^10.0.1",
    "handlebars": "^4.5.3",
    "make-dir": "^3.0.0",
    "ora": "^4.0.3",
    "p-each-series": "^2.1.0",
    "parse-git-config": "^3.0.0",
    "which": "^2.0.2",
    "yargs-interactive": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@favware/eslint-config": "^3.1.6",
    "@favware/ts-config": "^1.1.1",
    "@types/common-tags": "^1.8.0",
    "@types/git-config-path": "^2.0.0",
    "@types/jest": "^24.0.23",
    "@types/jsonfile": "^5.0.0",
    "@types/node": "^12.12.14",
    "@types/parse-git-config": "^3.0.0",
    "@types/which": "^1.3.2",
    "@types/yargs-interactive": "^2.1.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.7.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jsonfile": "^5.0.0",
    "lint-staged": "^9.5.0",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "files": [
    "dist",
    "templates"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
