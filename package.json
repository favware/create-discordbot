{
  "name": "create-djsbot",
  "version": "2.1.0",
  "description": "CLI for easily bootstrapping a DiscordJS bot with the Commando framework",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "bin": {
    "create-djsbot": "./dist/index.js"
  },
  "author": "@favware",
  "license": "MIT",
  "homepage": "https://favware.tech/create-djsbot",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/create-djsbot.git"
  },
  "bugs": {
    "url": "https://github.com/favware/create-djsbot/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint lib test --ext js,ts --fix",
    "test": "jest",
    "start": "ts-node ./lib/index.ts",
    "build": "tsc",
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "up": "yarn upgrade-interactive --latest",
    "standard": "yarn standard-version"
  },
  "keywords": [
    "discord",
    "create",
    "bootstrap",
    "commando",
    "favware"
  ],
  "dependencies": {
    "chalk": "^3.0.0",
    "execa": "^4.0.0",
    "git-config-path": "^2.0.0",
    "github-username": "^5.0.1",
    "globby": "^11.0.0",
    "handlebars": "^4.7.3",
    "make-dir": "^3.0.2",
    "ora": "^4.0.3",
    "p-each-series": "^2.1.0",
    "parse-git-config": "^3.0.0",
    "which": "^2.0.2",
    "yargs-interactive": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@favware/eslint-config": "^3.3.0",
    "@favware/ts-config": "^1.1.2",
    "@types/common-tags": "^1.8.0",
    "@types/git-config-path": "^2.0.0",
    "@types/jest": "^25.1.4",
    "@types/jsonfile": "^5.0.0",
    "@types/node": "^13.9.1",
    "@types/parse-git-config": "^3.0.0",
    "@types/which": "^1.3.2",
    "@types/yargs-interactive": "^2.1.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jsonfile": "^6.0.1",
    "lint-staged": "^10.0.8",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist",
    "templates"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged && yarn lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --ext ts"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "acorn": "^7.1.1",
    "minimist": "^1.2.2"
  }
}
